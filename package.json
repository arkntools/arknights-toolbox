{
  "name": "arknights-toolbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "update-data": "node ./tools/update.js",
    "update-data:local": "NODE_ENV=local node ./tools/update.js"
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^2.1.0",
    "core-js": "^3.6.4",
    "javascript-lp-solver": "^0.4.24",
    "js-base64": "^2.5.1",
    "lodash": "^4.17.15",
    "lodash.combinations": "^18.9.19",
    "md5": "^2.2.1",
    "mdui": "^0.4.3",
    "register-service-worker": "^1.6.2",
    "utf8-buffer-size": "0.0.4",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.3",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-pwa": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "cheerio": "^1.0.0-rc.3",
    "css-loader": "^3.4.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "fs-extra": "^8.1.0",
    "node-sass": "^4.13.1",
    "nodejieba": "^2.4.0",
    "pinyin": "^2.9.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-i18n": "^0.6.1",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "optionalDependencies": {
    "win-node-env": "^0.4.0"
  }
}
