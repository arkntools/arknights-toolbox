{
  "name": "arknights-toolbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:cdn": "cross-env USE_CDN=true vue-cli-service build && node tools/buildCDN.js",
    "update-data": "node ./tools/update.js",
    "update-data:cdn": "cross-env UPDATE_SOURCE=cdn node ./tools/update.js",
    "update-data:local": "cross-env UPDATE_SOURCE=local node ./tools/update.js"
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^2.1.0",
    "@yzfe/darkmodejs": "^1.2.1",
    "core-js": "^3.6.5",
    "javascript-lp-solver": "^0.4.24",
    "js-base64": "^2.5.2",
    "lodash": "^4.17.15",
    "lodash.combinations": "^18.9.19",
    "md5": "^2.2.1",
    "mdui": "^0.4.3",
    "register-service-worker": "^1.7.1",
    "utf8-buffer-size": "0.0.4",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.7",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-pwa": "^4.3.1",
    "@vue/cli-plugin-router": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "cache-loader": "^4.1.0",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "dateformat": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^9.0.0",
    "lint-staged": "^10.2.6",
    "node-sass": "^4.14.1",
    "nodejieba": "^2.4.1",
    "pinyin": "^2.9.1",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-i18n": "^1.0.1",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "prettier --write",
      "git add"
    ]
  }
}
